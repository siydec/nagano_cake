<div class="container">
  <div class="row mt-5">
    <div class="col">
      <table class="table mt-5">
        <thead>
          <tr>
          <th>商品名</th>
          <th>単価(税込)</th>
          <th>数量</th>
          <th>小計</th>
          <th></th>
          </tr>
        </thead>
         <% @cart_items.each do |cart_item| %>
        <tbody>
          <tr>
            <td><%= cart_item.item_id %></td>
            <td></td>
            <td><%= cart_item.amount %></td>
            <td></td>
          </tr>
        </tbody>
        <% end %>
      </table>

    </div>
  </div>
</div>